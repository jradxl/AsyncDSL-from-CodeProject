      <xsd:extension base="FlowElement">
        <!-- Id -->
        <xsd:attribute name="Id" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Instance Guid of this element, needed because SerializeId is set to true.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- Finish Moniker -->
  <xsd:element name="finishMoniker" type="FinishMoniker" substitutionGroup="flowElementMoniker" />
  <xsd:complexType name="FinishMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for Finish instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="FlowElementMoniker">
        <xsd:attribute name="Id" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation>Attribute to store the moniker string.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- ExampleModelHasProcesses -->
  <xsd:element name="exampleModelHasProcesses" abstract="true" type="ExampleModelHasProcesses" substitutionGroup="core:modelElement" />
  <xsd:complexType name="ExampleModelHasProcesses" abstract="true" >
    <xsd:complexContent>
      <xsd:extension base="core:ModelElement">
        <xsd:sequence minOccurs="0" maxOccurs="1">
          <!-- Target end Process -->
          <xsd:element ref="process" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation>[Target role-player]</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- ExampleModelHasProcesses Moniker -->
  <xsd:element name="exampleModelHasProcessesMoniker" abstract="true" type="ExampleModelHasProcessesMoniker" substitutionGroup="core:modelElementMoniker" />
  <xsd:complexType name="ExampleModelHasProcessesMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for ExampleModelHasProcesses instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="core:ModelElementMoniker">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- ProcessHasFlowElements -->
  <xsd:element name="processHasFlowElements" abstract="true" type="ProcessHasFlowElements" substitutionGroup="core:modelElement" />
  <xsd:complexType name="ProcessHasFlowElements" abstract="true" >
    <xsd:complexContent>
      <xsd:extension base="core:ModelElement">
